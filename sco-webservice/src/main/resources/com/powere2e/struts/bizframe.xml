<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
  <package name="bizframe" extends="pubstruts" namespace="/">
    <action name="org" method="{1}" class="com.powere2e.bizframe.org.OrgAction">
      <interceptor-ref name="mydefault"/>
    </action>
    <action name="retailer" method="{1}" class="com.powere2e.bizframe.retailer.RetailerAction">
      <interceptor-ref name="mydefault"/>
    </action>
    <action name="supplier" method="{1}" class="com.powere2e.bizframe.supplier.SupplierAction">
     	<interceptor-ref name="mydefault"/>
    </action>
    <action name="relationSupplierRetailer" method="{1}" class="com.powere2e.bizframe.relation.RelationSupplierRetailerAction">
      <interceptor-ref name="mydefault"/>
    </action>
  </package>
</struts>
